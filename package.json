{
  "name": "btld-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "playwright-test test/specification/*.js --runner mocha --watch",
    "bench": "playwright-test test/benchmark/bench1.js --runner benchmark",
    "build": "node esbuild.mjs",
    "tdd-code": "npm run build -- --watch",
    "tdd-test": "npm run build -- --watch --type=test --outfile=abc",
    "tdd": "concurrently -P \"npm:tdd-* -- {*}\" -- literate/0",
    "build-watch": "concurrently --kill-others \"node esbuild.mjs -w\" \"npm run test\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.7",
    "concurrently": "^7.6.0",
    "esbuild": "0.17.11",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "playwright-test": "^8.2.0"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2"
  }
}
