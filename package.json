{
  "name": "btld-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "literate": "node build/build-literate.mjs",
    "literate-w": "npm run literate -- --watch",
    "types": "npm run literate && tsc -p build",
    "bundle": "node build/build-bundle.mjs --watch --test",
    "test": "playwright-test test/specification/*.js --runner mocha --watch",
    "bench": "playwright-test test/benchmark/bench1.js --runner benchmark",
    "tdd-code": "npm run build -- literate/0-state.md --watch --tdd",
    "tdd-test": "playwright-test dist/tdd.js --runner mocha --watch --cov",
    "tdd": "concurrently --kill-others 'npm:tdd-*'",
    "build-watch": "concurrently --kill-others \"node esbuild.mjs --watch\" \"npm run test\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.3.7",
    "chokidar": "^3.5.3",
    "concurrently": "^7.6.0",
    "esbuild": "0.17.11",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "playwright-test": "^8.2.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.2"
  }
}
